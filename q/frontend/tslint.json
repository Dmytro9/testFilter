{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {
    "no-unused-expression": true,
    "object-literal-sort-keys": false
  },
  "rules": {
    "curly": [
      true,
      "ignore-same-line"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "max-line-length": [
      true,
      150
    ],
    "member-ordering": [
      false
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "no-any": true,
    "no-shadowed-variable": false,
    "jsx-no-multiline-js": false
  },
  "rulesDirectory": []
}